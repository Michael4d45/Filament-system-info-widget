{
    "name": "michael4d45/filament-system-info-widget",
    "description": "A Filament widget for displaying system information including package versions and deployment details",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Michael4d45\\FilamentSystemInfo\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Michael",
            "email": "your.email@example.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "filament/filament": "^3.0|^4.0",
        "laravel/framework": "^10.0|^11.0|^12.0",
        "spatie/laravel-package-tools": "^1.0",
        "nesbot/carbon": "^2.0|^3.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0|^9.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Michael4d45\\FilamentSystemInfo\\FilamentSystemInfoServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}